  <META name="date" content="2008-07-03T08:49:37+00:00" />

<%= stylesheet_link_tag "webportal" , :media => "all" %>
<%= stylesheet_link_tag "galleryview" , :media => "all" %>
<%= javascript_include_tag 'jquery-1.4.2.js',  'galleryview-2.1.1/jquery.easing.1.3.js', 'galleryview-2.1.1/jquery.galleryview-2.1.1.js', 'galleryview-2.1.1/jquery.timers-1.2.js' %>

<script type="text/javascript"> 
	$(document).ready(function(){
		$('#filmstrip').galleryView({
			panel_width: 800,
			panel_height: 300,
			frame_width: 100,
			frame_height: 95
		});
	});
</script> 
</head>

<body>
<div id="banner" >
<%= @page_title || "Portal Web para pequeños negocios" %>
<label class="beta">Versión beta</label>
</div>
<div id="columns" >
 <div id="menuside">
 </div>
<div id="main" >
  <% if flash[:notice] -%>
    <div id="notice" ><%= flash[:notice] %></div>
  <% end -%>
  <%= yield :layout %>
</div>
</div>
</body>
</html>